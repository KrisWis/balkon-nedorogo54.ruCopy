\/100)*(o_calc.h\/100)\/3;\r\n\t\t\t\t\t\tvar itog=stvorka*calc_settings.gluh+stvorka*calc_settings.pov+stvorka*calc_settings.gluh; break;}\t\r\n\t\tcase 'm3_2' : { var stvorka=(o_calc.v\/100)*(o_calc.h\/100)\/3;\r\n\t\t\t\t\t\tvar itog=stvorka*calc_settings.gluh+stvorka*calc_settings.pov+stvorka*calc_settings.pov; break;}\t\r\n\t\tcase 'm3_3' : { var stvorka=(o_calc.v\/100)*(o_calc.h\/100)\/3;\r\n\t\t\t\t\t\tvar itog=stvorka*calc_settings.pov_otk+stvorka*calc_settings.pov+stvorka*calc_settings.gluh; break;}\t\r\n\t\tcase 'm3_4' : { var stvorka=(o_calc.v\/100)*(o_calc.h\/100)\/3;\r\n\t\t\t\t\t\tvar itog=stvorka*calc_settings.pov_otk+stvorka*calc_settings.gluh+stvorka*calc_settings.gluh; break;}                                            \r\n\t\tcase 'm4_1' : { var itog=(o_calc.v\/100)*(o_calc.h\/100)*calc_settings.gluh; break;}\t\r\n\t\t\r\n\r\n\t}\r\n\r\n        var otliv = 0;\r\n        var montaz = 0;\r\n\r\n        if (o_calc.otliv === 1){\r\n            otliv = (o_calc.v\/100) * 390;\r\n        }\r\n        if (o_calc.montaz === 1){\r\n            montaz = (o_calc.v\/100) * (o_calc.h \/ 100) *800;\r\n        }\r\n\t\r\n        var itogo = itog + otliv + montaz;\r\n        var rassr = itogo \/ 6;\r\n        \r\n\t$('.itogo span').html(itogo.toFixed());\r\n\t$('.vrassr span').html(rassr.toFixed());\r\n\t\r\n}\r\n\r\n\r\n$(function(){\r\n\to_calculate();\r\n\t\r\n\t$('.o_submenu .mmm').on (\"click\",function(){\r\n\t\tif ($(this).hasClass('active')){\r\n\t            $('.o_submenu').removeClass('select');\r\n                    $(this).parent().parent().addClass('select');\r\n                    return \r\n\t\t}\r\n\t\t\r\n\t\tif ($(this).hasClass('sh')){ \r\n                    if (!$(this).parent().parent().hasClass('select')){\r\n                        $('.o_submenu').removeClass('select');\r\n        \t\t$(this).parent().parent().addClass('select');\r\n\t\t\treturn \r\n                    }\r\n\t\t}\r\n\t\t\r\n\t\t$('.o_submenu li.mmm').removeClass('active');\r\n\t\t$(this).addClass('active');\r\n                $(this).parent().find('li').removeClass('sh');\r\n                $(this).addClass('sh');\r\n\r\n\t\to_calc.selected=$(this).attr('id');\r\n\r\n\t\t$(this).parent().parent().removeClass('select');\r\n                \r\n                $('.maket img').attr('src','\/img\/calc_okna\/'+o_calc.selected+'.png');\r\n      o_calculate();          \r\n\t})\r\n\r\n        $('.dp_block .radio1').on (\"click\",function(){\r\n           var t = $(this).attr('id');\r\n           var val = t.substring(2);\r\n           var dest = $(this).parent().attr('id');\r\n           $('#'+dest+' .radio1').removeClass('active');\r\n           $(this).addClass('active');\r\n           \r\n           o_calc[dest]=parseInt(val);\r\n           o_calculate();\r\n           \r\n        })\r\n\r\n       \r\n       var handle_h = $( \".calc .vert .ui-slider-handle\" );\r\n       $( \"#v_slider\" ).slider({\r\n      value: 150,\r\n      orientation: \"vertical\",\r\n      max: 220,\r\n      min: 100,\r\n      range: \"max\",\r\n      animate: true,\r\n      create: function() {\r\n        handle_h.text( $( this ).slider( \"value\" ) + ' \u0441\u043c' );\r\n      },\r\n      slide: function( event, ui ) {\r\n        handle_h.text( ui.value + ' \u0441\u043c');\r\n        o_calc.v= ui.value;\r\n\to_calculate();\r\n    }\r\n    });\r\n     \r\n     var handle_v = $( \".calc .horiz .ui-slider-handle\" );\r\n       $( \"#h_slider\" ).slider({\r\n      value: 150,\r\n      orientation: \"horizontal\",\r\n      max: 300,\r\n      min: 80,\r\n      range: \"max\",\r\n      animate: true,\r\n       create: function() {\r\n        handle_v.text( $( this ).slider( \"value\" ) + ' \u0441\u043c' );\r\n      },\r\n      slide: function( event, ui ) {\r\n        handle_v.text( ui.value + ' \u0441\u043c');\r\n        o_calc.h= ui.value;\r\n\t\to_calculate();\r\n      }\r\n    });\r\n     \r\n    })\r\n<\/script>\r\n\r\n\n\n\r\n<div class=\"section z_price\">\r\n 