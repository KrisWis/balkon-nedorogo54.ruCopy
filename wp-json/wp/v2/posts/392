){\r\n            otliv = (o_calc.v\/100) * 390;\r\n        }\r\n        if (o_calc.montaz === 1){\r\n            montaz = (o_calc.v\/100) * (o_calc.h \/ 100) *400;\r\n        }\r\n\t\r\n        var itogo = itog + otliv + montaz;\r\n        var rassr = itogo \/ 6;\r\n        \r\n\t$('.itogo span').html(itogo.toFixed());\r\n\t$('.vrassr span').html(rassr.toFixed());\r\n\r\n}\r\n\r\n\r\n$(function(){\r\n\to_calculate();\r\n\t\r\n    $('.b_type').on('click',function(){\r\n       o_calc.selected=$(this).attr('id');\r\n       $('.b_type').removeClass('active');\r\n       $(this).addClass('active');\r\n        $('.maket img').attr('src','\/img\/calc_balkon\/'+o_calc.selected+'.png');\r\n       \r\n       o_calculate();\r\n    })\r\n\r\n        $('.dp_block .radio1').on (\"click\",function(){\r\n           var t = $(this).attr('id');\r\n           var val = t.substring(2);\r\n           var dest = $(this).parent().attr('id');\r\n           $('#'+dest+' .radio1').removeClass('active');\r\n           $(this).addClass('active');\r\n           \r\n           o_calc[dest]=parseInt(val);\r\n           o_calculate();\r\n        })\r\n\r\n       \r\n       var handle_h = $( \".calc .vert .ui-slider-handle\" );\r\n       $( \"#v_slider\" ).slider({\r\n      value: 150,\r\n      orientation: \"vertical\",\r\n      max: 200,\r\n      min: 100,\r\n      range: \"max\",\r\n      animate: true,\r\n      create: function() {\r\n        handle_h.text( $( this ).slider( \"value\" ) + ' \u0441\u043c' );\r\n      },\r\n      slide: function( event, ui ) {\r\n        handle_h.text( ui.value + ' \u0441\u043c');\r\n        o_calc.v= ui.value;\r\n\to_calculate();\r\n    }\r\n    });\r\n     \r\n     var handle_v = $( \".calc .horiz .ui-slider-handle\" );\r\n       $( \"#h_slider\" ).slider({\r\n      value: 150,\r\n      orientation: \"horizontal\",\r\n      max: 700,\r\n      min: 120,\r\n      range: \"max\",\r\n      animate: true,\r\n       create: function() {\r\n        handle_v.text( $( t